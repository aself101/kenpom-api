{
  "name": "kenpom-api",
  "version": "1.0.4",
  "description": "KenPom.com API wrapper - College basketball statistics for Node.js with CLI",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "type": "module",
  "bin": {
    "kenpom": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./parsers": {
      "types": "./dist/parsers.d.ts",
      "import": "./dist/parsers.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "kenpom": "node dist/cli.js",
    "kenpom:help": "node dist/cli.js --help",
    "kenpom:ratings": "node dist/cli.js --ratings",
    "kenpom:efficiency": "node dist/cli.js --efficiency",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "kenpom",
    "college-basketball",
    "ncaa",
    "basketball",
    "statistics",
    "sports-data",
    "scraper",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.18.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "cloudscraper": "^4.6.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^25.0.0",
    "@vitest/coverage-v8": "^4.0.10",
    "semantic-release": "^24.2.9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  },
  "optionalDependencies": {
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
